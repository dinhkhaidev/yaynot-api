{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "IS_SERVERLESS": "true"
  },
  "regions": ["sin1"],
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
